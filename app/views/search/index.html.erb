<%= @stations.count %> total
<% @stations.each do |station| %>
<ul class="station">
  <li class="name"><%= station.name %></li>
  <li class="address"><%= station.address %></li>
  <li class="fuel_types"><%= station.fuel_types %></li>
  <li class="distance"><%= station.distance %></li>
  <li class="access_time"><%= station.access_time %></li>
</ul>
<% end %>
